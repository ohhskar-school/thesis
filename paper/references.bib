@article{norman1982,
	title = {Why {Alphabetic} {Keyboards} {Are} {Not} {Easy} {To} {Use}:  {Keyboard} {Layout} {Doesn}'t {Much} {Matter}.},
	volume = {24},
	issn = {0018-7208},
	shorttitle = {{WHY} {ALPHABETIC} {KEYBOARDS} {ARE} {NOT} {EASY} {TO} {USE}},
	doi = {10.1177/001872088202400502},
	abstract = {These studies demonstrate the inferiority of alphabetically organized keyboards as compared with a randomly organized keyboard and the standard Sholes (qwerty) keyboard. Use of the alphabetic keyboard requires considerable mental processing. Comparison of different keyboard layouts by computer simulation of expert typing shows suprisingly little effect of keyboard arangement for a wide class of keyboards. Performance with some alphabetic layouts is quite slow, but with others, it is with 2\% of the speed achieved when using the Sholes keyboard. Performance with the Dvorak keyboard is not improved by about 5\% over performance on the Sholes keyboard. The conclusion is that is is not worthwhile to use alphabetic keyboards for novice typists, nor to change to the Dvorak layout for experts. Keyboards can probably be improved, but only through radical redesign of the present physical key configuration. Refs.},
	language = {English},
	number = {5},
